{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "deploy": "npm run build && npm run build:worker-asset-manifest && npm run build && wrangler kv:bulk put --binding __STATIC_CONTENT -e production assets.json && wrangler publish -e production",
    "dev:remix": "remix watch",
    "dev:miniflare": "cross-env NODE_ENV=development miniflare ./build/index.js --watch --site ./public",
    "dev": "remix build && run-p \"dev:*\"",
    "start": "cross-env NODE_ENV=production miniflare ./build/index.js",
    "build:worker-asset-manifest": "node scripts/generate-asset-files.mjs"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^1.6.8",
    "@remix-run/cloudflare-workers": "^1.6.8",
    "@remix-run/react": "^1.6.8",
    "cross-env": "^7.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "recursive-readdir": "^2.2.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.14.1",
    "@remix-run/dev": "^1.6.8",
    "@remix-run/eslint-config": "^1.6.8",
    "eslint": "^8.20.0",
    "miniflare": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "wrangler": "^2.0.22"
  },
  "engines": {
    "node": ">=14"
  }
}
